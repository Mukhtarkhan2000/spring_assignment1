<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <bean id="userAccount1"
          class="org.example.UserAccount">
        <constructor-arg index="0" value="User1"/>
        <constructor-arg index="1" value="1001"/>
        <constructor-arg index="2" value="1111"/>
        <constructor-arg index="3" value="0"/>
    </bean>
    <bean id="userAccount2"
          class="org.example.UserAccount">
        <constructor-arg index="0" value="User2"/>
        <constructor-arg index="1" value="1002"/>
        <constructor-arg index="2" value="2222"/>
        <constructor-arg index="3" value="0"/>
    </bean>

    <bean id="administratorAccount1"
          class="org.example.AdministratorAccount">
        <constructor-arg index="0" value="Administrator1"/>
        <constructor-arg index="1" value="0001"/>
        <constructor-arg index="2" value="1111"/>
        <constructor-arg index="3" value="0"/>
    </bean>
    <bean id="administratorAccount2"
          class="org.example.AdministratorAccount">
        <constructor-arg index="0" value="Administrator1"/>
        <constructor-arg index="1" value="0002"/>
        <constructor-arg index="2" value="2222"/>
        <constructor-arg index="3" value="0"/>
    </bean>

    <bean id="accountList"
          class="org.example.AccountList">
    </bean>
</beans>